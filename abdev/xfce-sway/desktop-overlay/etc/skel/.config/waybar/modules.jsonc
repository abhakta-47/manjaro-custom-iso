        "format": " {icon}{} ",
        "return-type": "json",
        "format-icons": {
            "Playing": "ÔÅå",
            "Paused": "ÔÅã",
        },
        "max-length": 70,
        // "exec": "playerctl -a metadata --format '{\"text\": \"{{playerName}}: {{markup_escape(title)}} -{{artist}} \", \"tooltip\": \"{{playerName}} : {{markup_escape(title)}}\", \"alt\": \"{{status}}\", \"class\": \"{{status}}\"}' -F",
